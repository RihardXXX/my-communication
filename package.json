{
    "name": "my-communication",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "test:unit": "vue-cli-service test:unit",
        "test:e2e": "vue-cli-service test:e2e",
        "lint": "vue-cli-service lint",
        "format": "prettier --write .",
        "fix": "npm run format",
        "android": "rm -R android && npm run build && npx cap add android && npx cap open android && npx cap run android -l --external",
        "ios": "rm -R ios && npm run build && npx cap add ios && npx cap open ios && npx cap run ios",
        "android-device": "rm -R android && npm run build && ionic cap run android -l --external",
        "ios-device": "rm -R android && npm run build && ionic cap run ios -l --external"
    },
    "dependencies": {
        "@capacitor/android": "4.4.0",
        "@capacitor/app": "4.1.0",
        "@capacitor/core": "4.4.0",
        "@capacitor/haptics": "4.0.1",
        "@capacitor/ios": "^4.4.0",
        "@capacitor/keyboard": "4.0.1",
        "@capacitor/status-bar": "4.0.1",
        "@ionic/storage": "^3.0.6",
        "@ionic/vue": "^6.0.0",
        "@ionic/vue-router": "^6.0.0",
        "axios": "^1.1.3",
        "cordova-sqlite-storage": "^6.0.0",
        "core-js": "^3.6.5",
        "ionicons": "^6.0.3",
        "localforage-cordovasqlitedriver": "^1.8.0",
        "pinia": "^2.0.23",
        "socket.io-client": "^4.5.3",
        "vue": "^3.2.21",
        "vue-axios": "^3.5.0",
        "vue-router": "^4.0.12"
    },
    "devDependencies": {
        "@capacitor/cli": "4.4.0",
        "@types/jest": "^27.0.2",
        "@typescript-eslint/eslint-plugin": "^5.6.0",
        "@typescript-eslint/parser": "^5.6.0",
        "@vue/cli-plugin-babel": "^5.0.8",
        "@vue/cli-plugin-e2e-cypress": "~5.0.0-rc.1",
        "@vue/cli-plugin-eslint": "~5.0.0-rc.1",
        "@vue/cli-plugin-router": "~5.0.0-rc.1",
        "@vue/cli-plugin-typescript": "~5.0.0-rc.1",
        "@vue/cli-plugin-unit-jest": "~5.0.0-rc.1",
        "@vue/cli-service": "~5.0.0-rc.1",
        "@vue/eslint-config-typescript": "^9.1.0",
        "@vue/test-utils": "^2.0.0-rc.16",
        "@vue/vue3-jest": "^27.0.0-alpha.3",
        "babel-jest": "^27.3.1",
        "cypress": "^8.7.0",
        "eslint": "^8.4.1",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-vue": "^8.2.0",
        "jest": "^27.3.1",
        "prettier": "^2.7.1",
        "ts-jest": "^27.0.7",
        "typescript": "^4.3.5"
    },
    "description": "An Ionic project"
